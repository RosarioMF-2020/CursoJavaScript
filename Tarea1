<!-- Tarea1: Programa que imprime valores del 1 al 100, excepto los multiplos de 3, 7 u 8 -->
<!-- Fecha: 10 - enero - 2022 -->
<head>
	<title>Prueba JavaScript</title>
</head>
<body>
	<script language="Javascript">
		let i = 0
		for (i=1; i<=100;i++){
		let m3=0
		let m7=0
		let m8=0
		m3=i % 3
		m7=i % 7
		m8=i % 8	
		if(m3!=0 && m7!=0 && m8!=0){
			console.log(i)
		}
	}
	</script>
